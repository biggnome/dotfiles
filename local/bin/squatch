#!/usr/bin/env bash

# ANSI color scheme script featuring Squatch

# Original: http://crunchbanglinux.org/forums/post/126921/#p126921
# Modified by me

f=3 b=4
for j in f b; do
  for i in {0..7}; do
    printf -v $j$i %b "\e[${!j}${i}m"
  done
done
bld=$'\e[1m'
rst=$'\e[0m'

cat << EOF

$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⠟⣫⣭⣍⢻⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡟⣱⣾⠻⠿⢿⢸⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⠿⠿⣛⣭⣾⣿⠿⠙⡲⢢⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⢋⣴⣾⣿⣿⣿⣿⠛⠿⠒⢠⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⢋⣴⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⢸⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⢋⣴⣿⣿⣿⢿⣿⣿⣿⣿⣿⣿⣿⡟⢸⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⠟⣡⣾⣿⣿⠟⣡⡆⣿⡿⠛⣻⣿⣿⣿⡇⠸⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⡿⢋⣴⣿⣿⡿⢋⣥⣾⣿⣿⣮⣙⣛⣉⠸⠿⠉⣰⣷⡌⢻⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⢋⣴⣿⡿⢛⡅⣴⣿⣿⣿⣿⣿⣿⣿⣿⣿⢇⡐⢿⣿⣿⣿⣦⡙⢿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⠟⣱⣿⡿⢟⡴⢋⣾⣿⣿⣿⣿⣿⣿⣿⣿⠟⣥⣿⣿⣶⣬⡙⢿⣿⣿⣆⢻⣿⣿$rst
$bld$f2⣿⠟⠛⣡⣾⠟⣫⣴⡿⢡⣿⣿⣿⣿⣿⣿⣿⣿⣿⢃⣾⣿⣿⣿⣿⣿⣿⣶⣌⡛⢻⣦⡙⢿$rst
$bld$f2⣿⠸⠿⢛⣡⣾⣿⣿⠃⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣎⢻⣿⣿⣿⣿⣿⣿⣿⣿⣿⡆⢽⠟⢠$rst
$bld$f2⣿⣷⣶⣿⣿⣿⣿⣿⠀⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣆⢻⣿⣿⣿⣿⣿⣿⣿⣿⣿⣶⣾⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣧⠙⢿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⠈⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⣿⢸⣷⣬⣙⡛⠿⣿⣿⣿⣿⣿⣿⡇⢻⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⡿⢸⣿⣿⣿⣿⡷⢨⣙⠿⣿⣿⣿⣿⣄⢻⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⣿⣿⠃⣿⣿⣿⣿⠟⣰⣿⣿⠇⣽⣿⣿⣿⡿⢸⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⣿⣿⠟⣡⣾⣿⣿⣿⠏⣼⣿⣿⣿⠀⣿⣿⣿⣿⢃⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⣿⣿⡟⣡⣾⣿⣿⣿⠿⢋⣼⣿⣿⣿⣿⡆⣿⣿⣿⡟⢸⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣿⡿⢋⣴⣿⣿⡿⢋⣥⣾⣿⣿⣿⣿⣿⣿⣧⢸⣿⣿⡇⣾⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⡿⢋⣴⣿⣿⠟⣡⣾⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⢸⣿⣿⡇⢿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⡀⠻⣿⣿⣁⣚⠛⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⢃⣼⣿⣿⣿⣌⣙⠻⢿⣿⣿⣿⣿⣿⣿⣿⣿$rst
$bld$f2⣿⣦⣄⣉⣉⣥⣾⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣦⣍⣛⣛⣉⣉⣛⠛⠒⣂⣽⣿⣿⣿⣿⣿⣿$rst

EOF
